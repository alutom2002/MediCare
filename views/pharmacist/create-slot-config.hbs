<section class="admin-add ml-250">
  <div class="head">
    <h1>
      Tạo ca thời gian
    </h1>
  </div>
  <div class="error">
    {{error}}
  </div>
  <div class="container">
    <form action="/config/slot" method="POST">
      <div class="input-section">
        <input type="time" name="startTime" required />
        <label for="startTime">
          Thời gian làm việc từ
        </label>
      </div>
      <div class="input-section">
        <input type="time" name="endTime" required />
        <label for="endTime">
          Thời gian làm việc tới
        </label>
      </div>
      <div class="input-section">
        <input type="submit" value="Gửi" />
      </div>
    </form>
  </div>
</section>