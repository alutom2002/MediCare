<section class="dr-page ptnt ml-250">

    <div class="user-container">
        <div id="doctor-container" class="container active">
            <div class="doctors-display" style="padding: 25px;">
                <table>
                    <thead>
                        <tr>
                            <th>
                                Tên bệnh nhân
                            </th>
                            <th>
                                Bác sĩ khám
                            </th>
                            <th>
                                Người lập đơn
                            </th>
                            <th>
                                Ngày lập đơn
                            </th>
                            <th>
                                Thuốc
                            </th>
                             <th>
                                Thành tiền
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each receipts}}
                        <tr id="{{this._id}}">
                            <td>
                                {{this.user.name}}
                            </td>
                            <td>
                                {{this.doctor.name}}
                            </td>
                            <td>
                                {{this.receptionist.name}}
                            </td>
                            <td>
                                {{this.date}}
                            </td>
                            <td>
                                {{this.medicines}}
                            </td>
                            <td>
                                {{this.total}}
                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
<script src="/javascripts/patient-search.js"></script>